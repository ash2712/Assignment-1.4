public class MyClass
{
	public static void main(String s[])
			{
				char letters[] = {'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};
				int num = 65;
				for(int i=0;i<letters.length;i++ )
				{
					System.out.println(letters[i]  + " = " + num); 
					num++;
				}
			}
}
